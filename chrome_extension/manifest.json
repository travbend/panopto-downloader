{
    "name": "Panopto Downloader",
    "description": "Download videos from Panopto.",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "js": ["scripts/ffmpeg.js", "scripts/ffmpegutil.js", "scripts/content.js"],
            "matches": [
                "https://*.hosted.panopto.com/Panopto/Pages/*"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["scripts/handle-click.js", "scripts/ffmpeg.js", "scripts/814.ffmpeg.js", "scripts/ffmpegutil.js"],
            "matches": ["<all_urls>"]
        }
    ]
  }